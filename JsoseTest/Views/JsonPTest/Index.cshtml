@{
    ViewBag.Title = "Index";
}
<head>
    <title></title>
    <script src="~/js/jquery-1.8.2.js"></script>
    <script type="text/javascript">
        $(function () {
            $.ajax({
                Type: "GET",
                url: "http://localhost:2843/api/TestApi/GetAll",
                //GetAll会转向GetAllContacts 使用了jsonp
                dataType: "jsonp",
                success: function (date) {
                    //alert(JSON.stringify(date));
                    $("#showDate").text(JSON.stringify(date));
                  
                }
            });
            $.getJSON("http://localhost:2843/api/TestApi/GetAll", function (data) {
                GetBackData(data);
            });
        });

        function GetBackData(data) {
            alert(JSON.stringify(data));
        }
    </script>
</head>
<body>
    @*<script type="text/javascript" src="http://localhost:3721/api/TestApi?callback=GetBackData"></script>*@
    <h2>Index</h2>
    <label id="showDate"></label>
</body>