<!--
  有关如何配置 ASP.NET 应用程序的详细信息，请访问
  http://go.microsoft.com/fwlink/?LinkId=301880
  -->
<configuration>
  <!--serviceStack配置 4.0以上版本每小时6000条 所以最好使用StackExchange-->
  <configSections>
    <section name="RedisConfig" type="GemmyTest.RedisUtil.RedisConfig, GemmyTest"/>
  </configSections>
  <RedisConfig WriteServerConStr="127.0.0.1:6379,192.168.3.135:6379" ReadServerConStr="127.0.0.1:6379,192.168.3.135:6379" MaxWritePoolSize="60"
       MaxReadPoolSize="60" AutoStart="true" LocalCacheTime="180" RecordeLog="false">
  </RedisConfig>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
     <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
  </appSettings>
  <connectionStrings>
    <add name="RedisExchangeHosts" connectionString="127.0.0.1:6379,allowadmin=true" />
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
</configuration>
